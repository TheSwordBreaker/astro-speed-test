<script>
	document.addEventListener("DOMContentLoaded", function () {
		var scrollStart = 0;
		var navbarElement = document.querySelector("nav");
		const offset = navbarElement?.getBoundingClientRect();

		document.onscroll = function (e: any) {
			// if (navbarElement.className.includes('shadow-sm')) return;
			if (!e.target?.scrollingElement || !offset) return;

			scrollStart = e.target.scrollingElement.scrollTop;
			if (scrollStart > offset.top) {
				console.log("add");
				navbarElement?.classList.add("shadow");
			} else {
				navbarElement?.classList.remove("shadow");
			}
		};
	});
</script>
